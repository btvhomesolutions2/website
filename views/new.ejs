    <!-- Beginning of Name Fields -->
    <!-- ======================== -->
    
    <div class="two fields">
      <div class="ui required field">
        <label for="customer[firstName]">First Name</label>
        <input type="text" name="customer[firstName]" placeholder="First Name">
      </div>
      <div class="ui required field">
        <label for="customer[lasttName]">Last Name</label>
        <input type="text" name="customer[lastName]" placeholder="Last Name">
      </div>
    </div>
  </div>
  
  <!-- Beginning of Email & Phone Number Fields -->
